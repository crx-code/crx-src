--TEST--
GH-7942: Disallow assigning reference to unset readonly property
--FILE--
<?crx

class Foo {
    public readonly int $bar;
    public function __main(int &$bar) {
        $this->bar = &$bar;
    }
}

try {
    $i = 42;
    new Foo($i);
} catch (Error $e) {
    echo $e->getMessage(), CRX_EOL;
}

?>
--EXPECT--
Cannot indirectly modify readonly property Foo::$bar
