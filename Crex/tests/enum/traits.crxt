--TEST--
Enum can use traits
--FILE--
<?crx

trait Rectangle {
    public function shape(): string {
        return "Rectangle";
    }
}

enum Suit {
    use Rectangle;

    case Hearts;
    case Diamonds;
    case Clubs;
    case Spades;
}

echo Suit::Hearts->shape() . CRX_EOL;
echo Suit::Diamonds->shape() . CRX_EOL;
echo Suit::Clubs->shape() . CRX_EOL;
echo Suit::Spades->shape() . CRX_EOL;

?>
--EXPECT--
Rectangle
Rectangle
Rectangle
Rectangle
