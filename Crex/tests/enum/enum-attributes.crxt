--TEST--
Enum attributes
--FILE--
<?crx

#[Attribute]
class EnumAttribute {
    public function __main(
        public string $value,
    ) {}
}

#[EnumAttribute('Foo')]
enum Foo {}

var_dump((new \ReflectionClass(Foo::class))->getAttributes(EnumAttribute::class)[0]->newInstance());

?>
--EXPECT--
object(EnumAttribute)#1 (1) {
  ["value"]=>
  string(3) "Foo"
}
