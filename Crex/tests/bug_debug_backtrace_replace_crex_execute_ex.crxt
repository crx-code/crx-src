--TEST--
bug_debug_backtrace.crxt with replaced crex_execute_ex
--EXTENSIONS--
crex_test
--INI--
crex_test.replace_crex_execute_ex=1
opcache.jit=disable
--FILE--
<?crx
function foo() {
    bar();
}

function bar() {
    boo();
}

function boo(){
    debug_print_backtrace();
}

eval("foo();");

echo "Done\n";
?>
--EXPECTF--
#0 %s(%d): boo()
#1 %s(%d): bar()
#2 %s(%d) : eval()'d code(1): foo()
#3 %s(%d): eval()
Done
