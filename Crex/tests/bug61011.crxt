--TEST--
Bug #61011 (Crash when an exception is thrown by autoloader accessing a static property)
--FILE--
<?crx
spl_autoload_register(function ($name) {
    throw new Exception($name);
});

try {
    echo AAA::$a; //crex_fetch_var_address_helper
} catch (Exception $e) {
    try {
        echo AAA::XXX; //CREX_FETCH_CONSTANT
    } catch (Exception $e) {
        try {
            echo AAA::foo(); //CREX_INIT_STATIC_METHOD_CALL
        } catch (Exception $e) {
            try  {
                unset(AAA::$a); // CREX_UNSET_VAR
            } catch (Exception $e){
                try {
                    isset(AAAA::$a); // CREX_ISSET_ISEMPTY_VAR
                } catch (Exception $e) {
                    try  {
                        $a = array("AAA", "foo");
                        $a(); //CREX_INIT_FCALL_BY_NAME
                    } catch (Exception $e) {
                    }
                }
            }
        }
    }
}
echo 'okey';
--EXPECT--
okey
