--TEST--
Inheritance of union type from internal class
--EXTENSIONS--
crex_test
--FILE--
<?crx

class C extends _CrexTestClass {}

$obj = new _CrexTestChildClass;
$obj->classUnionProp = new stdClass;
$obj->classUnionProp = new ArrayIterator;
try {
    $obj->classUnionProp = new DateTime;
} catch (TypeError $e) {
    echo $e->getMessage(), "\n";
}

$obj = new C;
$obj->classUnionProp = new stdClass;
$obj->classUnionProp = new ArrayIterator;
try {
    $obj->classUnionProp = new DateTime;
} catch (TypeError $e) {
    echo $e->getMessage(), "\n";
}

?>
--EXPECT--
Cannot assign DateTime to property _CrexTestClass::$classUnionProp of type stdClass|Iterator|null
Cannot assign DateTime to property _CrexTestClass::$classUnionProp of type stdClass|Iterator|null
