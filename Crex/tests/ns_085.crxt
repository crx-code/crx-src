--TEST--
085: bracketed namespace
--FILE--
<?crx
namespace foo {
use \foo;
class bar {
    function __main() {echo __METHOD__,"\n";}
}
new foo;
new bar;
}
namespace {
class foo {
    function __main() {echo __METHOD__,"\n";}
}
use foo\bar as foo1;
new foo1;
new foo;
echo "===DONE===\n";
}
?>
--EXPECT--
foo::__main
foo\bar::__main
foo\bar::__main
foo::__main
===DONE===
