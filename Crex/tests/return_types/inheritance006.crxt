--TEST--
External covariant return type of self
--INI--
opcache.enable_cli=1
--FILE--
<?crx
require __DIR__ . "/classes.crx.inc";

class Foo {
    public static function test() : A {
        return new A;
    }
}

class Bar extends Foo {
    public static function test() : B {
        return new B;
    }
}

echo get_class(Bar::test());

?>
--EXPECT--
B
