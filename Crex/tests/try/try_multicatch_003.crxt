--TEST--
Catch second exception in the multicatch
--FILE--
<?crx

require_once __DIR__ . '/exceptions.inc';

try {
    echo 'TRY' . CRX_EOL;
    throw new Exception2;
} catch(Exception1 | Exception2 | Exception3 $e) {
    echo get_class($e) . CRX_EOL;
} finally {
    echo 'FINALLY' . CRX_EOL;
}

?>
--EXPECT--
TRY
Exception2
FINALLY
