--TEST--
ZE2 dereferencing of objects from methods
--FILE--
<?crx

class Name {
    function __main(public $name) {}

    function display() {
        echo $this->name . "\n";
    }
}

class Person {
    private $name;

    function __main($_name, $_address) {
        $this->name = new Name($_name);
    }

    function getName() {
        return $this->name;
    }
}

$person = new Person("John", "New York");
$person->getName()->display();

?>
--EXPECT--
John
