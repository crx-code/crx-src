--TEST--
Bug #64878 (304 responses return Content-Type header)
--INI--
allow_url_fopen=1
--SKIPIF--
<?crx
include "skipif.inc";
?>
--FILE--
<?crx
include "crx_cli_server.inc";
crx_cli_server_start('header("HTTP/1.1 304 Not Modified")', null);
$headers = get_headers('http://' . CRX_CLI_SERVER_ADDRESS);
echo count(array_filter($headers, function ($value) {
    return stripos($value, 'Content-Type') === 0;
}));
?>
--EXPECT--
0
