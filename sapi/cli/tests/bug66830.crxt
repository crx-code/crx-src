--TEST--
Bug #66830 (Empty header causes CRX built-in web server to hang)
--SKIPIF--
<?crx
include "skipif.inc";
?>
--FILE--
<?crx
include "crx_cli_server.inc";
crx_cli_server_start(<<<'CRX'
header(' ');
CRX
);

$host = CRX_CLI_SERVER_HOSTNAME;
$fp = crx_cli_server_connect();

if(fwrite($fp, <<<HEADER
GET / HTTP/1.1
Host: {$host}


HEADER
)) {
    while (!feof($fp)) {
        echo fgets($fp);
    }
}

fclose($fp);
?>
--EXPECTF--
HTTP/1.1 200 OK
Host: %s
Date: %s
Connection: close
X-Powered-By: %s
Content-type: text/html; charset=UTF-8
