--SKIPIF--
<?crx
include "skipif.inc";
?>
--FILE--
<?crx
include "crx_cli_server.inc";
crx_cli_server_start('header(\'WWW-Authenticate: Digest realm="foo",qop="auth",nonce="XXXXX",opaque="'.md5("foo").'"\');');

$host = CRX_CLI_SERVER_HOSTNAME;
$fp = crx_cli_server_connect();

if(fwrite($fp, <<<HEADER
GET / HTTP/1.1
Host: {$host}
Authorization: Basic Zm9vOmJhcg==


HEADER
)) {
    while (!feof($fp)) {
        echo fgets($fp);
    }
}

?>
