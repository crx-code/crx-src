--TEST--
FPM: Bug #78323 Test exit code for invalid parameters
--SKIPIF--
<?crx include "skipif.inc"; ?>
--FILE--
<?crx

require_once "tester.inc";

$crx = \FPM\Tester::findExecutable();

// no argument for option
ob_start();
passthru("$crx --memory-limit=1G 2>&1", $exitCode);
$output = ob_get_contents();
ob_end_clean();

$lines = preg_split('/\R/', $output);
echo $lines[0], "\n",
     "Done: $exitCode\n\n";


// Successful execution
ob_start();
passthru("$crx -dmemory-limit=1G -v", $exitCode);
$output = ob_get_contents();
ob_end_clean();

$lines = preg_split('/\R/', $output);
echo $lines[0], "\n",
     "Done: $exitCode\n";

?>
--EXPECTF--
Usage: %s
Done: 64

CRX %s
Done: 0
