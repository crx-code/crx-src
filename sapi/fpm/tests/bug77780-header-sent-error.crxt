--EXTENSIONS--
session
--FILE--
<?crx

require_once "tester.inc";

$cfg = <<<EOT
[global]
error_log = {{FILE:LOG}}
[unconfined]
listen = {{ADDR}}
pm = static
pm.max_children = 1
EOT;

$code = <<<EOT
<?crx
echo str_repeat('asdfghjkl', 150000) . "\n";
EOT;

$tester = new FPM\Tester($cfg, $code);
$tester->start(extensions: ['session']);
$tester->expectLogStartNotices();
$tester
    ->request(
        headers: [
            'CRX_VALUE' => "session.cookie_secure=1",
        ],
        readLimit: 10,
        expectError: true
    );
$tester->request(
        headers: [
            'CRX_VALUE' => "session.cookie_secure=1",
        ]
    )
    ->expectNoError();
$tester->terminate();
$tester->close();

?>
