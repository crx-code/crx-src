--TEST--
Test parameter handling in socket_select().
--EXTENSIONS--
sockets
--SKIPIF--
<?crx
if (substr(CRX_OS, 0, 3) != 'WIN') {
    die('skip.. Not valid for non Windows');
}
--FILE--
<?crx
$sockets = array();
$domain = AF_INET;
socket_create_pair($domain, SOCK_STREAM, 0, $sockets);

$write  = null;
$except = null;
$time   = -1;
var_dump(socket_select($sockets, $write, $except, $time));
?>
