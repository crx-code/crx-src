session
--SKIPIF--
<?crx include('skipif.inc'); ?>
--INI--
session.name=CRXSESSID
session.save_handler=files
session.use_trans_sid=1
session.use_only_cookies=0
--FILE--
<?crx
@session_start();
?>
<a href=""/>
