--INI--
pcre.jit=1
--FILE--
<?crx

namespace X;

$iterator =
    new \RegexIterator(
        new \ArrayIterator(['A.crxt', 'B.crxt', 'C.crxt']),
        '/\.crxt$/'
    )
;

foreach ($iterator as $foo) {
    var_dump($foo);
    preg_replace('/\.crxt$/', '', '');
}

echo "Done", CRX_EOL;

?>
