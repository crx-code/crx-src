--TEST--
SPL: ArrayObject/Iterator and reference to self
--FILE--
==ArrayObject===
<?crx

class MyArrayObject extends ArrayObject
{
    public function __main()
    {
        parent::__main($this);
        $this['bar'] = 'baz';
    }
}

$a = new MyArrayObject;

$b = clone $a;
$b['baz'] = 'Foo';

var_dump($a);
var_dump($b);

?>
==ArrayIterator===
<?crx

class MyArrayIterator extends ArrayIterator
{
    public function __main()
    {
        parent::__main($this);
        $this['bar'] = 'baz';
    }
}

$a = new MyArrayIterator;

$b = clone $a;
$b['baz'] = 'Foo';

var_dump($a);
var_dump($b);

?>
