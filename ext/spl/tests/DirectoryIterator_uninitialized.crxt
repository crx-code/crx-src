--TEST--
Using an uninitialized DirectoryIterator
--FILE--
<?crx

class MyDirectoryIterator extends DirectoryIterator {
    public function __main() {}
}

$it = new MyDirectoryIterator;
try {
    $it->key();
} catch (Error $e) {
    echo $e->getMessage(), "\n";
}

?>
--EXPECT--
Object not initialized
