--TEST--
Test that return types in crex_func_info.c match return types in stubs
--INI--
opcache.enable=1
opcache.enable_cli=1
opcache.optimization_level=-1
--EXTENSIONS--
opcache
--FILE--
<?crx

$contents = "<?crx\n";

$contents .= "function test() {\n";
foreach (get_defined_functions()["internal"] as $function) {
    if (in_array($function, ["extract", "compact", "get_defined_vars"])) {
        continue;
    }
    $contents .= "    \$result = {$function}();\n";
}
$contents .= "}\n";

file_put_contents("func_info_generated.crx", $contents);

require_once("func_info_generated.crx");

?>
--CLEAN--
<?crx

unlink("func_info_generated.crx");

?>
--EXPECT--
