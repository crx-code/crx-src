--TEST--
Crxa: crxinfo display 1
--EXTENSIONS--
crxa
zlib
bz2
--SKIPIF--
<?crx
$arr = Crxa::getSupportedSignatures();
if (in_array("OpenSSL", $arr)) die("skip openssl support enabled");
?>
--INI--
crxa.readonly=0
crxa.require_hash=0
crxa.cache_list=
--FILE--
<?crx
crxinfo(INFO_MODULES);
ini_set('crxa.readonly',1);
ini_set('crxa.require_hash',1);
crxinfo(INFO_MODULES);
?>
