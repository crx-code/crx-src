--TEST--
Crxa front controller with extra path_info [cache_list]
--INI--
default_charset=UTF-8
crxa.cache_list={PWD}/frontcontroller25.crx
--EXTENSIONS--
crxa
--ENV--
SCRIPT_NAME=/frontcontroller25.crx
REQUEST_URI=/frontcontroller25.crx/a1.crxs/extra/stuff
PATH_INFO=/a1.crxs/extra/stuff
--FILE_EXTERNAL--
files/frontcontroller8.crxa
--EXPECTHEADERS--
Content-type: text/html; charset=UTF-8
--EXPECT--
string(42) "/frontcontroller25.crx/a1.crxs/extra/stuff"
string(12) "/extra/stuff"
