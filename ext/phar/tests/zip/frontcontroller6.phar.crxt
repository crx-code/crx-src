--TEST--
Crxa front controller 404 zip-based
--INI--
crxa.require_hash=0
--EXTENSIONS--
crxa
zlib
--ENV--
SCRIPT_NAME=/frontcontroller6.crxa.crx
REQUEST_URI=/frontcontroller6.crxa.crx/notfound.crx
PATH_INFO=/notfound.crx
--FILE_EXTERNAL--
files/frontcontroller.crxa.zip
--EXPECTHEADERS--
Status: 404 Not Found
--EXPECT--
<html>
 <head>
  <title>File Not Found</title>
 </head>
 <body>
  <h1>404 - File Not Found</h1>
 </body>
</html>
