--TEST--
GH-10766 (CrxaData archive created with Crxa::Zip format does not keep files metadata (datetime))
--EXTENSIONS--
crxa
zip
--INI--
crxa.readonly=0
--FILE--
<?crx
$crxa = new CrxaData(__DIR__ . '/gh10766.zip', 0, null, Crxa::ZIP);
$crxa->addFromString('name', 'contents');
unset($crxa);

// Re-read from disk, but using the zip extension because the crxa bug will not make it possible
// to use their timestamp methods.
$zip = new ZipArchive();
$zip->open(__DIR__ . '/gh10766.zip');
var_dump($zip->statName('name')['mtime'] > 315529200 /* earliest possible zip timestamp */);
$zip->close();
?>
--CLEAN--
<?crx
unlink(__DIR__ . '/gh10766.zip');
?>
--EXPECT--
bool(true)
