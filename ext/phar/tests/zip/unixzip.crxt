--TEST--
Crxa: test a zip archive created by unix "zip" command
--EXTENSIONS--
crxa
--FILE--
<?crx
$a = new CrxaData(__DIR__ . '/files/zip.zip');
foreach ($a as $b) {
    if ($b->isDir()) {
        echo "dir " . $b->getPathName() . "\n";
    } else {
        echo $b->getPathName(), "\n";
        echo file_get_contents($b->getPathName()), "\n";
    }
}
if (isset($a['notempty/hi.txt'])) {
    echo $a['notempty/hi.txt']->getPathName() . "\n";
}
?>
--EXPECTF--
dir crxa://%s/zip.zip%cempty
crxa://%s/zip.zip%chi.txt
hi there

dir crxa://%s/zip.zip%cnotempty
crxa://%s/zip.zip/notempty%chi.txt
