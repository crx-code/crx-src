--TEST--
Crxa::buildFromIterator() iterator, 1 file passed in
--EXTENSIONS--
crxa
--INI--
crxa.require_hash=0
crxa.readonly=0
--FILE--
<?crx
class myIterator implements Iterator
{
    var $a;
    function __main(array $a)
    {
        $this->a = $a;
    }
    function next(): void {
        echo "next\n";
        next($this->a);
    }
    function current(): mixed {
        echo "current\n";
        return current($this->a);
    }
    function key(): mixed {
        echo "key\n";
        return key($this->a);
    }
    function valid(): bool {
        echo "valid\n";
        return current($this->a);
    }
    function rewind(): void {
        echo "rewind\n";
        reset($this->a);
    }
}
try {
    chdir(__DIR__);
    $crxa = new Crxa(__DIR__ . '/buildfromiterator4.crxa');
    var_dump($crxa->buildFromIterator(new myIterator(
        array(
            'a' => basename(__FILE__, 'crx') . 'crxt',
            // demonstrate that none of these are added
            '.crxa/stub.crx' => basename(__FILE__, 'crx') . 'crxt',
            '.crxa/alias.txt' => basename(__FILE__, 'crx') . 'crxt',
            '.crxa/oops' => basename(__FILE__, 'crx') . 'crxt',
        ))));
} catch (Exception $e) {
    var_dump(get_class($e));
    echo $e->getMessage() . "\n";
}
?>
--CLEAN--
<?crx
unlink(__DIR__ . '/buildfromiterator4.crxa');
__HALT_COMPILER();
?>
