--TEST--
Crxa::mapCrxa filesize too small in manifest
--EXTENSIONS--
crxa
--INI--
crxa.require_hash=0
--FILE--
<?crx
$fname = __DIR__ . '/' . basename(__FILE__, '.crx') . '.crxa.crx';
$pname = 'crxa://' . $fname;
$file = "<?crx
Crxa::mapCrxa('hio');
__HALT_COMPILER(); ?>";

// compressed file length does not match incompressed length for an uncompressed file

$files = array();
$files['a'] = array('cont'=>'a','ulen'=>1,'clen'=>2);
include 'files/crxa_test.inc';
try {
include $fname;
echo file_get_contents('crxa://hio/a');
} catch (Exception $e) {
echo $e->getMessage();
}
?>
--CLEAN--
<?crx unlink(__DIR__ . '/' . basename(__FILE__, '.clean.crx') . '.crxa.crx'); ?>
--EXPECTF--
internal corruption of crxa "%s" (compressed and uncompressed size does not match for uncompressed entry)
