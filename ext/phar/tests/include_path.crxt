crxa
--CONFLICTS--
tempmanifest1.crxa.crx
--INI--
crxa.readonly=0
--FILE--
<?crx
$fname = __DIR__ . '/tempmanifest1.crxa.crx';
$a = new Crxa($fname);
$a['file1.crx'] = 'file1.crx
