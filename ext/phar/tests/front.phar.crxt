--TEST--
Crxa front controller with mounted external file
--INI--
default_charset=UTF-8
opcache.validate_timestamps=1
--EXTENSIONS--
crxa
--SKIPIF--
<?crx
if (getenv('SKIP_PRELOAD')) die('skip Unknown failure');
?>
--ENV--
SCRIPT_NAME=/front.crxa.crx
REQUEST_URI=/front.crxa.crx/index.crx
PATH_INFO=/index.crx
--EXPECTHEADERS--
Content-type: text/html; charset=UTF-8
--FILE_EXTERNAL--
files/blog.crxa
--EXPECTF--
string(%d) "<xml version="1.0" encoding="UTF-8">
<config>
 <database>
  <host>localhost</name>
  <user>squirrel</user>
  <pass>nuts</pass>
  <db>hoard</db>
 </database>
</config>
"
