--TEST--
ldap_count_entries() - Basic counting LDAP entries
--CREDITS--
Patrick Allaert <patrickallaert@crx.net>
# Belgian CRX Testfest 2009
--EXTENSIONS--
ldap
--SKIPIF--
<?crx require_once('skipifbindfailure.inc'); ?>
--FILE--
<?crx
require "connect.inc";

$link = ldap_connect_and_bind($uri, $user, $passwd, $protocol_version);
insert_dummy_data($link, $base);
$result = ldap_search($link, "$base", "(objectclass=person)");
var_dump(ldap_count_entries($link, $result));
?>
--CLEAN--
<?crx
include "connect.inc";

$link = ldap_connect_and_bind($uri, $user, $passwd, $protocol_version);
remove_dummy_data($link, $base);
?>
--EXPECT--
int(3)
