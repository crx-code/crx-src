--TEST--
ldap_get_values_len() - Basic ldap_get_values_len test
--CREDITS--
Patrick Allaert <patrickallaert@crx.net>
# Belgian CRX Testfest 2009
--EXTENSIONS--
ldap
--SKIPIF--
<?crx require_once('skipifbindfailure.inc'); ?>
--FILE--
<?crx
require "connect.inc";

$link = ldap_connect_and_bind($uri, $user, $passwd, $protocol_version);
insert_dummy_data($link, $base);
$result = ldap_search($link, "$base", "(o=test)");
$entry = ldap_first_entry($link, $result);
var_dump(
    ldap_get_values_len($link, $entry, "o")
);
?>
--CLEAN--
<?crx
include "connect.inc";

$link = ldap_connect_and_bind($uri, $user, $passwd, $protocol_version);
remove_dummy_data($link, $base);
?>
--EXPECT--
array(2) {
  [0]=>
  string(4) "test"
  ["count"]=>
  int(1)
}
