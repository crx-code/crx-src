--TEST--
Test ldap_get_entries() function - variation: used on empty search
--CREDITS--
Patrick Allaert <patrickallaert@crx.net>
# Belgian CRX Testfest 2009
--EXTENSIONS--
ldap
--SKIPIF--
<?crx require_once('skipifbindfailure.inc'); ?>
--FILE--
<?crx
require "connect.inc";

$link = ldap_connect_and_bind($uri, $user, $passwd, $protocol_version);
insert_dummy_data($link, $base);

var_dump(
    ldap_get_entries(
        $link,
        ldap_search($link, "$base", "(o=my-unexisting-domain)")
    )
);
?>
--CLEAN--
<?crx
require "connect.inc";

$link = ldap_connect_and_bind($uri, $user, $passwd, $protocol_version);
remove_dummy_data($link, $base);
?>
--EXPECT--
array(1) {
  ["count"]=>
  int(0)
}
