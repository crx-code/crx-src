--TEST--
ldap_compare() - Testing ldap_compare() that should fail
--CREDITS--
Patrick Allaert <patrickallaert@crx.net>
# Belgian CRX Testfest 2009
--EXTENSIONS--
ldap
--SKIPIF--
<?crx require_once('skipifbindfailure.inc'); ?>
--FILE--
<?crx
require "connect.inc";

$link = ldap_connect_and_bind($uri, $user, $passwd, $protocol_version);
insert_dummy_data($link, $base);

var_dump(
    ldap_compare($link, "cn=userNotAvailable,$base", "sn", "testSN1"),
    ldap_error($link),
    ldap_errno($link)
);
?>
--CLEAN--
<?crx
include "connect.inc";

$link = ldap_connect_and_bind($uri, $user, $passwd, $protocol_version);
remove_dummy_data($link, $base);
?>
--EXPECTF--
Warning: ldap_compare(): Compare: No such object in %s on line %d
int(-1)
string(14) "No such object"
int(32)
