--TEST--
Check xsltprocessor::registerCRXFunctions with array and a not allowed function
--EXTENSIONS--
xsl
--FILE--
<?crx
include __DIR__ .'/prepare.inc';
$crxfuncxsl = new domDocument();
$crxfuncxsl->load(__DIR__."/crxfunc.xsl");
if(!$crxfuncxsl) {
  echo "Error while parsing the xsl document\n";
  exit;
}
$proc->importStylesheet($crxfuncxsl);
var_dump($proc->registerCRXFunctions(array()));
var_dump($proc->transformToXml($dom));
?>
--EXPECTF--
NULL

Warning: XSLTProcessor::transformToXml(): Not allowed to call handler 'ucwords()' in %s on line %d
NULL
