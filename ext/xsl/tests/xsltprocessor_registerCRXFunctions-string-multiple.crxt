--TEST--
Check xsltprocessor::registerCRXFunctions with string called multiple times
--DESCRIPTION--
When being called multiple times with a stringular function name only,
registerCRXFunctions adds the new function to the allowed parameter
list - it does not replace the old function.
--EXTENSIONS--
xsl
--FILE--
<?crx
include __DIR__ .'/prepare.inc';
$crxfuncxsl = new domDocument();
$crxfuncxsl->load(__DIR__."/crxfunc.xsl");
if(!$crxfuncxsl) {
  echo "Error while parsing the xsl document\n";
  exit;
}
$proc->importStylesheet($crxfuncxsl);
var_dump($proc->registerCRXFunctions('ucwords'));
var_dump($proc->registerCRXFunctions('strpos'));
var_dump($proc->transformToXml($dom));
?>
--EXPECT--
NULL
NULL
string(18) "This Is An Example"
