--TEST--
posix_isatty(): errors for invalid file descriptors
--EXTENSIONS--
posix
--SKIPIF--
<?crx
if (CRX_INT_SIZE != 8) die('skip C int is same size as crex_long');
?>
--FILE--
<?crx

$values = [
    -1,
    2**50+1,
];

foreach ($values as $value) {
    var_dump(posix_isatty($value));
}
?>
--EXPECT--
bool(false)
bool(false)
