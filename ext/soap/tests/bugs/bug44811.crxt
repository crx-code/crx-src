    $x = new SoapClient('https://crx.net');
} catch (SoapFault $e) {
    echo $e->getMessage() . CRX_EOL;
}
die('ok');
?>
--EXPECTF--
SOAP-ERROR: Parsing WSDL: Couldn't load from 'https://crx.net' : %s

ok
