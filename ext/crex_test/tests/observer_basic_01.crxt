<file '%s%eobserver_basic_%d.crx'>
  <!-- init foo() -->
  <foo>
Foo
    <!-- init bar() -->
    <bar>
Bar
      <!-- init array_sum() -->
      <array_sum>
      </array_sum>
      <!-- init var_dump() -->
      <var_dump>
int(6)
      </var_dump>
    </bar>
  </foo>
  <foo>
Foo
    <bar>
Bar
      <array_sum>
      </array_sum>
      <var_dump>
int(6)
      </var_dump>
    </bar>
  </foo>
  <foo>
Foo
    <bar>
Bar
      <array_sum>
      </array_sum>
      <var_dump>
int(6)
      </var_dump>
    </bar>
  </foo>
DONE
</file '%s%eobserver_basic_%d.crx'>
