--TEST--
Observer: Basic observability of userland functions
--EXTENSIONS--
crex_test
--INI--
crex_test.observer.enabled=1
crex_test.observer.observe_all=1
opcache.optimization_level=0x7FFFBFFF & ~0x0080
--FILE--
<?crx
function bar()
{
    echo 'Bar' . CRX_EOL;
    var_dump(array_sum([1,2,3]));
}

function foo()
{
    echo 'Foo' . CRX_EOL;
    bar();
}

foo();
foo();
foo();

echo 'DONE' . CRX_EOL;
?>
--EXPECTF--
