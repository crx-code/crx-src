--TEST--
Observer: Basic observability of functions only (with run-time swapping)
--EXTENSIONS--
crex_test
--INI--
crex_test.observer.enabled=1
crex_test.observer.observe_function_names=foo
--FILE--
<?crx
function foo()
{
    echo 'Foo' . CRX_EOL;
}

function bar()
{
    echo 'Bar' . CRX_EOL;
}

foo();
bar();

ini_set("crex_test.observer.observe_function_names", "bar");

foo();
bar();

?>
--EXPECTF--
<!-- init '%s%eobserver_basic_06.crx' -->
<!-- init foo() -->
<foo>
Foo
</foo>
<!-- init bar() -->
Bar
<!-- init ini_set() -->
Foo
<bar>
Bar
</bar>
