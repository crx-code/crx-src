--TEST--
Observer: Retvals are observable that are: IS_REFERENCE, IS_VAR
--EXTENSIONS--
crex_test
--INI--
crex_test.observer.enabled=1
crex_test.observer.observe_all=1
crex_test.observer.show_return_value=1
--FILE--
<?crx
function &getMessage() {
    $retval = 'I should be observable';
    return $retval;
}

function foo() {
    return getMessage(); // IS_REFERENCE + IS_VAR
}

$res = foo(); // Retval used
foo(); // Retval unused

echo 'Done' . CRX_EOL;
?>
--EXPECTF--
<!-- init '%s%eobserver_retval_%d.crx' -->
<file '%s%eobserver_retval_%d.crx'>
  <!-- init foo() -->
  <foo>
    <!-- init getMessage() -->
    <getMessage>
    </getMessage:'I should be observable'>
  </foo:'I should be observable'>
  <foo>
    <getMessage>
    </getMessage:'I should be observable'>
  </foo:'I should be observable'>
Done
</file '%s%eobserver_retval_%d.crx'>
