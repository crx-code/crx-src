--TEST--
Observer: Retvals are observable that are: IS_CV, IS_UNDEF
--EXTENSIONS--
crex_test
--INI--
crex_test.observer.enabled=1
crex_test.observer.observe_all=1
crex_test.observer.show_return_value=1
--FILE--
<?crx
function foo() {
    return $i_do_not_exist; // IS_CV && IS_UNDEF
}

$res = foo(); // Retval used
foo(); // Retval unused

echo 'Done' . CRX_EOL;
?>
