--TEST--
Observer: Basic generator observability
--EXTENSIONS--
crex_test
--INI--
crex_test.observer.enabled=1
crex_test.observer.observe_all=1
crex_test.observer.show_return_value=1
--FILE--
<?crx
function getResults() {
    for ($i = 10; $i < 13; $i++) {
        yield $i;
    }
}

function doSomething() {
    $generator = getResults();
    foreach ($generator as $value) {
        echo $value . CRX_EOL;
    }

    return 'Done';
}

echo doSomething() . CRX_EOL;
?>
--EXPECTF--
<!-- init '%s%eobserver_generator_%d.crx' -->
<file '%s%eobserver_generator_%d.crx'>
  <!-- init doSomething() -->
  <doSomething>
    <!-- init getResults() -->
    <getResults>
    </getResults:10>
10
    <getResults>
    </getResults:11>
11
    <getResults>
    </getResults:12>
12
    <getResults>
    </getResults:NULL>
  </doSomething:'Done'>
Done
</file '%s%eobserver_generator_%d.crx'>
