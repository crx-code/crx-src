--TEST--
Observer: Basic eval observability
--EXTENSIONS--
crex_test
--INI--
crex_test.observer.enabled=1
crex_test.observer.observe_all=1
--FILE--
<?crx
echo eval("return 'Foo eval' . CRX_EOL;");
echo 'DONE' . CRX_EOL;
?>
--EXPECTF--
