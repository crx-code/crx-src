--TEST--
Observer: Retvals are observable that are: refcounted, IS_CV
--EXTENSIONS--
crex_test
--INI--
crex_test.observer.enabled=1
crex_test.observer.observe_all=1
crex_test.observer.show_return_value=1
--FILE--
<?crx
class MyRetval {}

function foo() {
    $retval = new MyRetval(); // Refcounted
    return $retval; // IS_CV
}

$res = foo(); // Retval used
foo(); // Retval unused

echo 'Done' . CRX_EOL;
?>
--EXPECTF--
<!-- init '%s%eobserver_retval_%d.crx' -->
<file '%s%eobserver_retval_%d.crx'>
  <!-- init foo() -->
  <foo>
  </foo:object(MyRetval)#%d>
  <foo>
  </foo:object(MyRetval)#%d>
Done
</file '%s%eobserver_retval_%d.crx'>
