--TEST--
Observer: Retvals by reference are observable that are: IS_CV
--EXTENSIONS--
crex_test
--INI--
crex_test.observer.enabled=1
crex_test.observer.observe_all=1
crex_test.observer.show_return_value=1
--FILE--
<?crx
function &foo() {
    $retval = 'I should be observable';
    return $retval; // IS_CV
}

$res = foo(); // Retval used
foo(); // Retval unused

echo 'Done' . CRX_EOL;
?>
--EXPECTF--
