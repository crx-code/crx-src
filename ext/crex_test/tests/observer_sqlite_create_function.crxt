  <PDO::__main>
  </PDO::__main>
  <!-- init PDO::sqliteCreateFunction() -->
  <PDO::sqliteCreateFunction>
  </PDO::sqliteCreateFunction>
  <!-- init PDO::query() -->
  <PDO::query>
    <!-- init returnOne() -->
    <returnOne>
    </returnOne>
  </PDO::query>
  <!-- init var_dump() -->
