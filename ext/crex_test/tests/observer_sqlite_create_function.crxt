--TEST--
Observer: PDO::sqliteCreateFunction() can be observed
--EXTENSIONS--
crex_test
PDO
pdo_sqlite
--INI--
crex_test.observer.enabled=1
crex_test.observer.observe_all=1
--FILE--
<?crx

function returnOne() {
    return 1;
}

$db = new PDO('sqlite::memory:');
$db->sqliteCreateFunction('returnOne', 'returnOne', 0);

foreach ($db->query('SELECT returnOne()') as $row) {
    var_dump($row);
}

echo 'Done' . CRX_EOL;
?>
--EXPECTF--
