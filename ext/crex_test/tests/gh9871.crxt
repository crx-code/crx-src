--TEST--
Test observing inherited internal functions
--EXTENSIONS--
crex_test
--INI--
opcache.enable_cli=1
crex_test.observer.enabled=1
crex_test.observer.observe_all=1
crex_test.observer.show_return_value=1
--FILE--
<?crx

class MyReflectionProperty extends ReflectionProperty {
}

class A {
    protected $protected = 'a';
}

$property = new MyReflectionProperty('A', 'protected');
$property->setAccessible(true);

?>
--EXPECTF--
<!-- init '%s' -->
<file '%s'>
  <!-- init ReflectionProperty::__main() -->
  <ReflectionProperty::__main>
  </ReflectionProperty::__main:NULL>
  <!-- init ReflectionProperty::setAccessible() -->
  <ReflectionProperty::setAccessible>
  </ReflectionProperty::setAccessible:NULL>
</file '%s'>
