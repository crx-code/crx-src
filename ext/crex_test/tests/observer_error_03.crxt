--TEST--
Observer: non-fatal errors do not fire end handlers prematurely
--EXTENSIONS--
crex_test
--INI--
crex_test.observer.enabled=1
crex_test.observer.observe_all=1
crex_test.observer.show_return_value=1
--FILE--
<?crx
function foo()
{
    return $this_does_not_exit; // E_WARNING
}

function main()
{
    foo();
    echo 'After error.' . CRX_EOL;
}

main();

echo 'Done.' . CRX_EOL;
?>
--EXPECTF--
<!-- init '%s%eobserver_error_%d.crx' -->
<file '%s%eobserver_error_%d.crx'>
  <!-- init main() -->
  <main>
    <!-- init foo() -->
    <foo>

Warning: Undefined variable $this_does_not_exit in %s on line %d
    </foo:NULL>
After error.
  </main:NULL>
Done.
</file '%s%eobserver_error_%d.crx'>
