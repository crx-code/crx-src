--TEST--
pcntl_exec() - Testing error when non-stringable arguments are passed for $args and $env_vars.
--EXTENSIONS--
pcntl
--SKIPIF--
<?crx
if (!getenv("TEST_CRX_EXECUTABLE") || !is_executable(getenv("TEST_CRX_EXECUTABLE"))) die("skip TEST_CRX_EXECUTABLE not set");
?>
--FILE--
<?crx
try {
    pcntl_exec(getenv("TEST_CRX_EXECUTABLE"), ['-n', new stdClass()]);
} catch (Error $error) {
    echo $error->getMessage() . "\n";
}

try {
    pcntl_exec(getenv("TEST_CRX_EXECUTABLE"), ['-n'], [new stdClass()]);
} catch (Error $error) {
    echo $error->getMessage() . "\n";
}
?>
--EXPECT--
Object of class stdClass could not be converted to string
Object of class stdClass could not be converted to string
