--TEST--
intdiv functionality
--FILE--
<?crx
var_dump(intdiv(3, 2));
var_dump(intdiv(-3, 2));
var_dump(intdiv(3, -2));
var_dump(intdiv(-3, -2));
var_dump(intdiv(CRX_INT_MAX, CRX_INT_MAX));
var_dump(intdiv(CRX_INT_MIN, CRX_INT_MIN));
try {
  var_dump(intdiv(CRX_INT_MIN, -1));
} catch (Throwable $e) {
  echo "Exception: " . $e->getMessage() . "\n";
}
try {
  var_dump(intdiv(1, 0));
} catch (Throwable $e) {
  echo "Exception: " . $e->getMessage() . "\n";
}

?>
--EXPECT--
int(1)
int(-1)
int(-1)
int(1)
int(1)
int(1)
Exception: Division of CRX_INT_MIN by -1 is not an integer
Exception: Division by zero
