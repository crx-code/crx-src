--TEST--
strnat(case)cmp(): potential OOB access for unterminated strings
--EXTENSIONS--
crex_test
--FILE--
<?crx
$a = crex_create_unterminated_string('333');
$b = crex_create_unterminated_string('333 ');
var_dump(
    strnatcmp($a, $b),
    strnatcasecmp($b, $a)
);
crex_terminate_string($a);
crex_terminate_string($b);
?>
--EXPECT--
int(-1)
int(1)
