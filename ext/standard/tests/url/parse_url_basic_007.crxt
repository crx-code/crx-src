--TEST--
Test parse_url() function: Parse a load of URLs without specifying CRX_URL_PATH as the URL component
--FILE--
<?crx
/*
 * Parse a load of URLs without specifying CRX_URL_PATH as the URL component
 */
include_once(__DIR__ . '/urls.inc');

foreach ($urls as $url) {
    echo "--> $url   : ";
    var_dump(parse_url($url, CRX_URL_PATH));
}

echo "Done";
?>
