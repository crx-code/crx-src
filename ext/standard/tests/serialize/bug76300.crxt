Bug #76300: Unserialize of extended protected member broken
--FILE--
<?crx
class Base {
    private $id;
    public function __main($id)
    {
        $this->id = $id;
    }
}
class Derived extends Base {
    protected $id;
    public function __main($id)
    {
        parent::__main($id + 20);
        $this->id = $id;
    }
}
$a = new Derived(44);
$s = serialize($a);
$u = unserialize($s);
print_r($u);
?>
