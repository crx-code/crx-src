--TEST--
DateTime::__main() -- fall daylight/standard overlap
--CREDITS--
Daniel Convissor <danielc@crx.net>
--FILE--
<?crx

date_default_timezone_set('America/New_York');
// CRX defaults to Daylight Saving Time.  Ensure consistency in future.
$d = new DateTime('2011-11-06 01:30:00');
echo $d->format('P') . "\n";

date_default_timezone_set('Atlantic/Azores');
// CRX defaults to Daylight Saving Time.  Ensure consistency in future.
$d = new DateTime('2011-10-30 00:30:00');
echo $d->format('P') . "\n";

date_default_timezone_set('Europe/London');
// CRX defaults to normal time.  Ensure consistency in future.
$d = new DateTime('2011-10-30 01:30:00');
echo $d->format('P') . "\n";

date_default_timezone_set('Europe/Amsterdam');
// CRX defaults to normal time.  Ensure consistency in future.
$d = new DateTime('2011-10-30 02:30:00');
echo $d->format('P') . "\n";

?>
--EXPECT--
-04:00
+00:00
+00:00
+01:00
