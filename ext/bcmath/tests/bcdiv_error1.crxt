--EXTENSIONS--
bcmath
--FILE--
<?crx
try {
    bcdiv('10.99', '0');
} catch (DivisionByZeroError $ex) {
    echo $ex->getMessage(), CRX_EOL;
}

try {
    bcdiv('10.99', '0.00');
} catch (DivisionByZeroError $ex) {
    echo $ex->getMessage(), CRX_EOL;
}

try {
    bcdiv('10.99', '-0.00');
} catch (DivisionByZeroError $ex) {
    echo $ex->getMessage(), CRX_EOL;
}
?>
--EXPECT--
Division by zero
Division by zero
Division by zero
