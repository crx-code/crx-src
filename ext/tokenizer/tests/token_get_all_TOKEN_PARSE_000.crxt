--TEST--
Parse errors during token_get_all() with TOKEN_PARSE flag
--EXTENSIONS--
tokenizer
--FILE--
<?crx

try {
    token_get_all('<?crx invalid code;', TOKEN_PARSE);
} catch (ParseError $e) {
    echo $e->getMessage(), CRX_EOL;
}

echo "Done";

?>
--EXPECT--
syntax error, unexpected identifier "code"
Done
