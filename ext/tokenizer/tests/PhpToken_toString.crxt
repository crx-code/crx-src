--TEST--
CrxToken implements __toString()
--EXTENSIONS--
tokenizer
--FILE--
<?crx

$tokens = CrxToken::tokenize('<?crx echo "Hello ". $what;');
var_dump(implode($tokens));

var_dump($tokens[0] instanceof Stringable);
var_dump((string) $tokens[0]);
var_dump($tokens[0]->__toString());

?>
--EXPECT--
string(27) "<?crx echo "Hello ". $what;"
bool(true)
string(6) "<?crx "
string(6) "<?crx "
