--TEST--
Tokenization of only the <?crx tag
--EXTENSIONS--
tokenizer
--INI--
short_open_tag=0
--FILE--
<?crx

foreach (token_get_all("<?crx") as $token) {
    echo token_name($token[0]), "\n";
}
echo "\n";
foreach (token_get_all("Foobar<?crx") as $token) {
    echo token_name($token[0]), "\n";
}

?>
--EXPECT--
T_OPEN_TAG

T_INLINE_HTML
T_OPEN_TAG
