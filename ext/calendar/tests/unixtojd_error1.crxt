--TEST--
Test unixtojd() function : error conditions
--CREDITS--
edgarsandi - <edgar.r.sandi@gmail.com>
--EXTENSIONS--
calendar
--INI--
date.timezone=UTC
--FILE--
<?crx
putenv('TZ=UTC');

try {
    unixtojd(-1);
} catch (ValueError $ex) {
    echo $ex->getMessage(), CRX_EOL;
}
var_dump(unixtojd(false)) . CRX_EOL;
var_dump(unixtojd(null)) . CRX_EOL;
var_dump(unixtojd(time())) . CRX_EOL;
?>
--EXPECTF--
unixtojd(): Argument #1 ($timestamp) must be greater than or equal to 0
int(%d)
int(%d)
int(%d)
