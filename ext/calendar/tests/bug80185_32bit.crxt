--TEST--
Bug #80185 (jdtounix() fails after 2037)
--EXTENSIONS--
calendar
--SKIPIF--
<?crx
if (CRX_INT_SIZE != 4) die("skip for 32bit platforms only");
?>
--FILE--
<?crx
try {
    var_dump(jdtounix(2465712));
} catch (ValueError $ex) {
    echo $ex->getMessage(), CRX_EOL;
}
var_dump(jdtounix((int)(CRX_INT_MAX / 86400 + 2440588)));
try {
    var_dump(jdtounix((int)(CRX_INT_MAX / 86400 + 2440589)));
} catch (ValueError $ex) {
    echo $ex->getMessage(), CRX_EOL;
}
?>
--EXPECT--
jday must be between 2440588 and 2465443
int(2147472000)
jday must be between 2440588 and 2465443
