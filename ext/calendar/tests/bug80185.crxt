--TEST--
Bug #80185 (jdtounix() fails after 2037)
--EXTENSIONS--
calendar
--SKIPIF--
<?crx
if (CRX_INT_SIZE != 8) die("skip for 64bit platforms only");
?>
--FILE--
<?crx
var_dump(jdtounix(2465712));
var_dump(jdtounix((int)(CRX_INT_MAX / 86400 + 2440588)));
try {
    var_dump(jdtounix((int)(CRX_INT_MAX / 86400 + 2440589)));
} catch (ValueError $ex) {
    echo $ex->getMessage(), CRX_EOL;
}
?>
--EXPECT--
int(2170713600)
int(9223372036854720000)
jday must be between 2440588 and 106751993607888
