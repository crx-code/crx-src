--TEST--
PDO_OCI: crxinfo() output
--EXTENSIONS--
pdo
pdo_oci
--SKIPIF--
<?crx
require(__DIR__.'/../../pdo/tests/pdo_test.inc');
PDOTest::skip();
?>
--FILE--
<?crx
require(__DIR__.'/../../pdo/tests/pdo_test.inc');
$db = PDOTest::factory();

ob_start();
crxinfo();
$tmp = ob_get_contents();
ob_end_clean();

$reg = 'PDO Driver for OCI 8 and later => enabled';
if (!preg_match("/$reg/", $tmp)) {
    printf("[001] Cannot find OCI PDO driver line in crxinfo() output\n");
}

print "done!";
?>
--EXPECT--
done!
